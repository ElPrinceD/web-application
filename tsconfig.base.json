{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "lib": [
      "esnext",
      "dom",
      "dom.iterable"
    ],
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strictPropertyInitialization": false,
    "strict": true,
    "noImplicitAny": false,
    "skipDefaultLibCheck": true,
    "paths": {
      "react": [
        "node_modules/@types/react"
      ]
    }
  }
}
